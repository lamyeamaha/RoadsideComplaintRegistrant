<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ewubd.roadsidecomplaintregistrant">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoadsideComplaintRegistrant">
        <activity android:exported="true" android:name=".MainHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminLoginActivity" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".AdminSeeComplaintListActivity" />
        <activity android:name=".AdminUpdateComplaintStatusActivity" />
        <activity android:name=".ReferenceActivity" />
        <activity android:name=".SubmitComplaintActivity" />
        <activity android:name=".UserFeedbackActivity" />
        <activity android:name=".AdminSeeFeedbacklist" />
        <activity android:name=".CheckAreaActivity" />
        <activity android:name=".ParticularAreaProblemActivity" />





  <uses-library android:name="org.apache.http.legacy" android:required="false"/>

    </application>

</manifest>